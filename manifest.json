{
  "manifest_version": 3,
  "version": "1.0.1",
  "name": "Scraper",
  "description": "Scraper for coupon duniya",
  "permissions": ["activeTab", "storage", "scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.coupondunia.in/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "assets/S.png",
        "default_title":"Scraper for Coupon Duniya",
    "default_popup": "popup/popup.html"
  }
}

